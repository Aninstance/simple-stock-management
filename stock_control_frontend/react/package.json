  "description": "React frontend for Django REST Simple Stock Management application",
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "*",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest-enzyme": "^7.0.1",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "popper.js": "^1.14.6",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.2.0",
    "react-dom": "^16.6.3",
    "react-markdown": "^4.0.3",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.6.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "HTTPS=false BROWSER='google chrome' react-scripts start",
    "build:staging": "env-cmd .env.staging react-scripts build",
    "build:production": "env-cmd .env.production react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {}
}
