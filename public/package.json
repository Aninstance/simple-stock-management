{
  "author": "Dan Bright, productions@aninstance.com",
  "license": "GPLv3",
  "name": "stock_control-react-frontend",
  "version": "4.1.2",
  "private": true,
  "description": "React frontend for Django REST Simple Stock Management application",
  "main": "index.js",
  "dependencies": {
    "@csstools/normalize.css": "*",
    "@fortawesome/fontawesome-svg-core": "*",
    "@fortawesome/free-brands-svg-icons": "*",
    "@fortawesome/free-regular-svg-icons": "*",
    "@fortawesome/free-solid-svg-icons": "*",
    "@fortawesome/react-fontawesome": "*",
    "acorn": "*",
    "axios": "*",
    "bootstrap": "*",
    "env-cmd": "*",
    "jest-enzyme": "*",
    "jquery": "*",
    "js-cookie": "*",
    "lodash.clonedeep": "*",
    "moment": "*",
    "moment-timezone": "*",
    "npm-check-updates": "*",
    "popper.js": "*",
    "react": "*",
    "react-app-polyfill": "*",
    "react-dom": "*",
    "react-modal": "*",
    "react-router-dom": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "HTTPS=false PORT=3001 react-scripts start",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:docker": "env-cmd -f .env.docker react-scripts build",
    "test": "react-scripts test",
    "test:once": "CI=true react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.11.0",
    "eslint-plugin-react": "^7.21.5"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "yargs-parser": "^13.1.2",
    "ini": "^1.3.6"
  }
}
